{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/TicketQuantityModal.js",
    "src/lib/firebaseAdmin.js",
    "src/utils/generateAndSendTicketPDF.js",
    "src/services/user.service.js",
    "src/services/ticket.service.js",
    "src/services/taxes.service.js",
    "src/services/payments.service.js",
    "src/services/match.service.js",
    "src/services/abonement.service.js",
    "src/components/AbonnementContent.js",
    "src/app/connexion/page.js",
    "src/app/mot-de-passe-oublie/page.js",
    "src/app/inscription/page.js",
    "src/components/SideBar.js",
    "src/components/PaymentForm.js",
    "src/components/MatchContent.js",
    "src/components/CheckoutForm.js",
    "src/components/AuthRequiredModal.js",
    "src/components/MatchsList.js",
    "src/app/(protected)/profil/page.js",
    "src/components/OrderComponent.js",
    "src/app/(protected)/profil/achats/[id]/page.js",
    "src/components/SuccessContent.js",
    "src/app/(protected)/commande-reussi/page.js",
    "src/components/MatchsContent.js",
    "src/app/(protected)/matchs/[Id]/page.js",
    "src/components/CheckoutContent.jsx",
    "src/app/(protected)/checkout/page.jsx",
    "src/app/api/webhook/route.js",
    "src/app/api/payment-status/route.js",
    "src/app/api/create-payment-intent/route.js",
    "src/app/(protected)/layout.js",
    "src/components/CallToActionBanner.js",
    "src/components/AbonnementSection.js",
    "src/components/HomeBanner.js",
    "src/app/(protected)/page.js"
  ],
  "exclude": ["node_modules"]
}
